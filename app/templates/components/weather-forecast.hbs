<ul class='inline-list right'>
  {{#each days}}
    <li>
      <div class="label round">
        {{date-formatter time}}
      </div>
      {{icon-name icon}}
      <span class="high">high {{temperatureMax}}&deg;</span>
      <span class="low">low {{temperatureMin}}&deg;</span>
    </li>
  {{/each}}
</ul>
